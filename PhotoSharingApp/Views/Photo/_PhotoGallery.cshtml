
@model IEnumerable<PhotoSharingApp.Models.Photo>
@foreach (var item in Model)
{
    <h3>"@item.Title"</h3>
    if (item.PhotoFile != null)
    {
        <img width="200"
             src="@Url.Action("GetImage",
"Photo",
new { id = item.PhotoID })" />

    }

    <p>
        Created By:
        @Html.DisplayFor(
     models => item.Owner)
    </p>
    <p>
        Created On:
        @Html.DisplayFor(
     models => item.CreateDate)
    </p>
    @Html.ActionLink("Display", "Display",
    new { id = item.PhotoID })


}
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>_PhotoGallery</title>
</head>
<body>
    <div>
    </div>
</body>
</html>